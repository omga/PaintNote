<?xml version="1.0" encoding="utf-8"?>
<co.hatrus.andrew.paint.MaterialRippleLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    style="@style/RippleStyle"
    android:background="@drawable/selector_background_empty">
    <LinearLayout
        android:layout_marginLeft="@dimen/activity_horizontal_margin"
        android:layout_marginRight="@dimen/activity_horizontal_margin"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">
        <ImageView
            android:id="@+id/type_icon"
            android:layout_gravity="center"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:layout_marginTop="4dp"
            android:layout_marginBottom="4dp"/>
        <co.hatrus.andrew.paint.RobotoTextView
            android:layout_gravity="center"
            android:text="Aszxas "
            style="@style/TextViewAppearance.Headline1"
            android:id="@+id/title_listnote"/>
        </LinearLayout>

</co.hatrus.andrew.paint.MaterialRippleLayout>
